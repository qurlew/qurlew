_qurlew()
{
	local cur prev words cword
	_init_completion || return

	case $prev in
	-u|--url)
		_filedir
		return
		;;
	esac

	local short_opts="-u -v -h"
	local long_opts="--url --version --help"
	if [[ "$cur" == -* ]]; then
		COMPREPLY=( $( compgen -W "$short_opts $long_opts" -- "$cur" ) )
		return
	fi

	return
} &&
complete -F _qurlew qurlew
